{
  "emitters": {
    "direction": "none",
    "size": {
      "width": 0,
      "height": 0
    },
    "position": {
      "x": 50,
      "y": 50
    },
    "rate": {
      "delay": 0.1,
      "quantity": 1
    }
  },
  "particles": {
    "number": {
      "value": 0,
      "density": {
        "enable": true,
        "valueArea": 800
      }
    },
    "color": {
      "value": "#ffffff"
    },
    "shape": {
      "type": "star",
      "polygon": {
        "sides": 7
      }
    },
    "opacity": {
      "value": {
        "min": 0,
        "max": 0.5
      }
    },
    "size": {
      "value": {
        "min": 1,
        "max": 7
      },
      "random": true
    },
    "move": {
      "enable": true,
      "speed": 5,
      "direction": "none",
      "random": false,
      "size": true,
      "straight": true,
      "outModes": {
        "default": "destroy"
      }
    }
  },
  "interactivity": {
    "detectsOn": "canvas",
    "events": {
      "onClick": {
        "enable": true,
        "mode": "pause"
      },
      "resize": true
    }
  }
}
